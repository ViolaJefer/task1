def print_chars_and_return_sum(a, b, c, d):
    if 126 < (a or b) < 32 or 126 < (a and b) < 32:
        return -1
    if (c or d) > 30 or (c and d) > 30:
        return -1
    for i in range(d):
        if i < 1:
            print(chr(a) * c)
        if i < d - 2:
            print(chr(a), chr(b) * (c - 2), chr(a))
        if i == d - 1:
            print(chr(a) * c)
    print(c*d)
